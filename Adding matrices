#include<iostream>
using namespace std;
           
int main()
{

int m, n;
cin >> m >> n;
int i, j;
int mat1[m][n], mat2[m][n], mat3[m][n];
cout<<"Enter the first matrix: "<<endl;
for(i = 0; i < m; i++)
{
for(j = 0; j < n; j++)
cin >> mat1[i][j];
}
cout<<"Enter the second matrix: "<<endl;
for(i = 0; i < n; i++)
{
for(j = 0; j < n; j++)
cin >> mat2[i][j];
}

for(i = 0; i < m; i++)
{
for(j = 0; j < n; j++)
{
mat3[i][j] = mat1[i][j] + mat2[i][j];
}
}
cout<<endl;
cout<<"Added matrix is: "<<endl;
for(i = 0; i < m; i++)
{
for(j = 0; j < n; j++)
cout << mat3[i][j] <<" ";
cout << endl;
}

return 0;
}

     OUTPUT
3 3 
Enter the first matrix:
3 4 5
1 3 9
5 7 8 
Enter the second matrix:
5 6 8
1 2 3 
4 2 5 
Added matrix:
8 10 13
2 5 12
9 9 13
