#include <iostream>
using namespace std; 
 
int gcd(int a, int b) 
{ 
    if (a == 0) 
        return b; 
    return gcd(b % a, a); 
} 
 

int phi(unsigned int n) 
{ 
    unsigned int result = 1; 
    for (int i = 2; i < n; i++) 
        if (gcd(i, n) == 1) 
            result++; 
    return result; 
} 
 

int main() 
{ 
    int n; 
    cout<<"Enter the limit: ";
    cin>>n;
    for (int i = 1; i <= n; i++) 
        cout << "phi("<<i<<") = " << phi(i) << endl; 
    return 0; 
} 
      OUTPUT
Enter the limit: 10
phi(1) = 1
phi(2) = 1
phi(3) = 2
phi(4) = 2
phi(5) = 4
phi(6) = 2
phi(7) = 6
phi(8) = 4 
phi(9) = 6
phi(10) = 4
